    <event-label>JUNIPER-JS-IDP-MIB defined trap event: jnxJsIdpSignatureUpdate</event-label>
    <descr><![CDATA[
<p>A trap which signifies that a signature update has occurred.</p><table>
	<tr><td><b>

	jnxJsIdpRunningDetectorVersion</b></td><td>
	%parm[#1]%;</td><td><p></p></td></tr>
	<tr><td><b>

	jnxJsIdpSecurityPackageVersion</b></td><td>
	%parm[#2]%;</td><td><p></p></td></tr>
	<tr><td><b>

	jnxJsIdpLastSignatureUpdateTime</b></td><td>
	%parm[#3]%;</td><td><p></p></td></tr>
	<tr><td><b>

	jnxJsIdpSignatureUpdateStatus</b></td><td>
	%parm[#4]%;</td><td><p></p></td></tr></table>
	]]></descr>
    <logmsg dest="logndisplay" notify="true"><![CDATA[<p>
			jnxJsIdpSignatureUpdate trap received 
			jnxJsIdpRunningDetectorVersion=%parm[#1]% 
			jnxJsIdpSecurityPackageVersion=%parm[#2]% 
			jnxJsIdpLastSignatureUpdateTime=%parm[#3]% 
			jnxJsIdpSignatureUpdateStatus=%parm[#4]%</p>
		]]></logmsg>
