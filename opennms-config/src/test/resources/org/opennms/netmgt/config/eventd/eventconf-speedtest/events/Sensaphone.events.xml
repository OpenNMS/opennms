<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
<!-- event definitions for traps from Sensaphone IMS-4000 environmental monitors -->
<event>
	<mask>
		<maskelement>
			<mename>id</mename>
			<mevalue>.1.3.6.1.4.1.8338</mevalue>
		</maskelement>
		<maskelement>
			<mename>generic</mename>
			<mevalue>6</mevalue>
		</maskelement>
		<maskelement>
			<mename>specific</mename>
			<mevalue>1</mevalue>
		</maskelement>
	</mask>
	<uei>uei.opennms.org/vendor/Sensaphone/traps/environment-alarm-trap</uei>
	<event-label>SENSAPHONE-MIB defined trap event: environment-alarm-trap</event-label>
	<descr>
&lt;p&gt;Description of environmental input alarm&lt;/p&gt;&lt;table&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	ims-trap-string&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#1]%;&lt;/td&gt;&lt;td&gt;&lt;p;&gt;&lt;/p&gt;&lt;/td;&gt;&lt;/tr&gt;&lt;/table&gt;
	</descr>
		<logmsg dest='logndisplay'>&lt;p&gt;
			Sensaphone Event: environment-alarm-trap trap received 
			ims-trap-string=%parm[#1]%&lt;/p&gt;
		</logmsg>
	<severity>Warning</severity>
</event>
