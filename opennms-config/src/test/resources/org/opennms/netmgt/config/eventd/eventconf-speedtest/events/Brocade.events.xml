 portNum-0(1) portNum-1(2) portNum-2(3) portNum-3(4) portNum-4(5) portNum-5(6) portNum-6(7) 
 portNum-7(8) portNum-8(9) portNum-9(10) portNum-10(11) portNum-11(12) portNum-12(13) portNum-13(14) 
 portNum-14(15) portNum-15(16)&lt;/p&gt;
 &lt;/td;&gt;&lt;/tr&gt;&lt;/table&gt;
 </descr>
