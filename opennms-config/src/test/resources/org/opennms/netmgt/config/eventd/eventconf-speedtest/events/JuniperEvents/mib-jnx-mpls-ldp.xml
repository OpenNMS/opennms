	<tr><td><b>

	jnxMplsLdpSesState</b></td><td>
	%parm[#1]%;</td><td><p>
		nonexistent(1)
		initialized(2)
		openrec(3)
		opensent(4)
		operational(5)
	</p></td></tr>
	<tr><td><b>

	jnxMplsLdpSesDiscontinuityTime</b></td><td>
	%parm[#2]%;</td><td><p></p></td></tr>
	<tr><td><b>

	jnxMplsLdpSesStatsUnkMesTypeErrors</b></td><td>
	%parm[#3]%;</td><td><p></p></td></tr>
	<tr><td><b>

	jnxMplsLdpSesStatsUnkTlvErrors</b></td><td>
	%parm[#4]%;</td><td><p></p></td></tr></table>
	]]></descr>
    <logmsg dest="logndisplay" notify="true"><![CDATA[<p>
			jnxMplsLdpSessionUp trap received 
			jnxMplsLdpSesState=%parm[#1]% 
