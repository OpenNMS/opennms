<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
<!-- Start of Quintum Traps -->
<!-- Start of Quintum MIB: Critical Alert -->
<event>
 <mask>
  <maskelement>
   <mename>id</mename>
   <mevalue>.1.3.6.1.4.1.6618.2</mevalue>
  </maskelement>
  <maskelement>
   <mename>generic</mename>
   <mevalue>6</mevalue>
  </maskelement>
  <maskelement>
   <mename>specific</mename>
   <mevalue>4</mevalue>
  </maskelement>
 <varbind>
  <vbnumber>2</vbnumber> 
  <vbvalue>.1.3.6.1.4.1.6618.2.0.1</vbvalue> 
  </varbind>
</mask>
 <uei>uei.opennms.org/vendor/Quintum/traps/QuintumCriticalAlarm</uei>
 <event-label>Quintum - Critical Alarm</event-label>
 <descr>&lt;p&gt;Quintum IP Address - %parm[#3]%&lt;/p&gt;&lt;table&gt;
 &lt;tr&gt;&lt;td&gt;&lt;b&gt;
 %parm[all]%&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
 &lt;/td&gt;&lt;td&gt;&lt;p;&gt;&lt;/p&gt;&lt;/td;&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;b&gt;
 &lt;/b&gt;&lt;/td&gt;&lt;td&gt;
 &lt;/td&gt;&lt;td&gt;&lt;p;&gt;&lt;/p&gt;&lt;/td;&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;b&gt;
 &lt;/b&gt;&lt;/td&gt;&lt;td&gt;
 &lt;/td&gt;&lt;td&gt;&lt;p;&gt;&lt;/p&gt;&lt;/td;&gt;&lt;/tr&gt;&lt;/table&gt;
 </descr>
 <logmsg dest='logndisplay'>&lt;p&gt;Quintum Event - Critical Alarm - %parm[#3]%&lt;/p&gt;</logmsg>
 <severity>Critical</severity>
</event>
