    <event-label>APS-MIB defined trap event: apsEventSwitchover</event-label>
    <descr><![CDATA[
<p>An apsEventSwitchover notification is sent when the
value of an instance of apsChanStatusSwitchovers increments.</p><table>
	<tr><td><b>

	apsChanStatusSwitchovers</b></td><td>
	%parm[#1]%;</td><td><p></p></td></tr>
	<tr><td><b>

	apsChanStatusCurrent</b></td><td>
	%parm[#2]%;</td><td><p>
		lockedOut(0)
		sd(1)
		sf(2)
		switched(3)
		wtr(4)
	</p></td></tr></table>
	]]></descr>
    <logmsg dest="logndisplay" notify="true"><![CDATA[<p>
			apsEventSwitchover trap received 
			apsChanStatusSwitchovers=%parm[#1]% 
			apsChanStatusCurrent=%parm[#2]%</p>
		]]></logmsg>
    <severity>Normal</severity>
    <alarm-data auto-clean="true" alarm-type="2" reduction-key="%uei%:%dpname%:%nodeid%:%parm[#1]%:%parm[#2]%"/>
