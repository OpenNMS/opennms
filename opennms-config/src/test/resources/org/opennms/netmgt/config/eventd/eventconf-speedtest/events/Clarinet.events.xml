<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
<!-- event definitions for traps from Clarinet IRDA switch mib -->
<event>
	<mask>
		<maskelement>
			<mename>id</mename>
			<mevalue>.1.3.6.1.4.1.2741.2</mevalue>
		</maskelement>
		<maskelement>
			<mename>generic</mename>
			<mevalue>6</mevalue>
		</maskelement>
		<maskelement>
			<mename>specific</mename>
			<mevalue>1</mevalue>
		</maskelement>
	</mask>
	<uei>uei.opennms.org/vendor/Clarinet/traps/heartBeat</uei>
	<event-label>CSI-P2-MIB defined trap event: heartBeat</event-label>
	<descr>
&lt;p&gt;This trap is the heart beat of the switch.&lt;/p&gt;&lt;table&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	swSerialNumber&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#1]%;&lt;/td&gt;&lt;td&gt;&lt;p;&gt;&lt;/p&gt;&lt;/td;&gt;&lt;/tr&gt;&lt;/table&gt;
	</descr>
		<logmsg dest='logndisplay'>&lt;p&gt;
			Clarinet Event: heartBeat trap received 
			swSerialNumber=%parm[#1]%&lt;/p&gt;
		</logmsg>
	<severity>Indeterminate</severity>
</event>
<event>
	<mask>
		<maskelement>
			<mename>id</mename>
			<mevalue>.1.3.6.1.4.1.2741.2</mevalue>
		</maskelement>
		<maskelement>
			<mename>generic</mename>
			<mevalue>6</mevalue>
		</maskelement>
		<maskelement>
			<mename>specific</mename>
			<mevalue>2</mevalue>
		</maskelement>
	</mask>
	<uei>uei.opennms.org/vendor/Clarinet/traps/fatal</uei>
	<event-label>CSI-P2-MIB defined trap event: fatal</event-label>
	<descr>
&lt;p&gt;This trap indicating a fatal condition.  Device will reboot.&lt;/p&gt;&lt;table&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	swSerialNumber&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#1]%;&lt;/td&gt;&lt;td&gt;&lt;p;&gt;&lt;/p&gt;&lt;/td;&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	fatalMessage&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#2]%;&lt;/td&gt;&lt;td&gt;&lt;p;&gt;&lt;/p&gt;&lt;/td;&gt;&lt;/tr&gt;&lt;/table&gt;
	</descr>
		<logmsg dest='logndisplay'>&lt;p&gt;
			Clarinet Event: fatal trap received 
			swSerialNumber=%parm[#1]% 
			fatalMessage=%parm[#2]%&lt;/p&gt;
		</logmsg>
	<severity>Indeterminate</severity>
</event>
</events>
