<!-- Start of 5300dchan Traps -->

<!-- Start of 5300dchan-PRI Down Trap -->
<event>
 <mask>
  <maskelement>
   <mename>id</mename>
   <mevalue>.1.3.6.1.4.1.9.9.26.2</mevalue>
  </maskelement>
  <maskelement>
   <mename>generic</mename>
   <mevalue>6</mevalue>
  </maskelement>
  <maskelement>
   <mename>specific</mename>
   <mevalue>3</mevalue>
  </maskelement>
  <varbind>
   <vbnumber>2</vbnumber>
   <vbvalue>1</vbvalue>
  </varbind>
 </mask>
 <uei>uei.opennms.org/vendor/C5300/traps/5300DChanDown</uei>
 <event-label>C5300-DChanDown</event-label>
	<descr>
&lt;p&gt;D Channel Down.&lt;/p&gt;&lt;table&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	ifIndex&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#1]%;&lt;/td&gt;&lt;td&gt;&lt;p;&gt;&lt;/p&gt;&lt;/td;&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	isdnLapdOperStatus&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#2]%;&lt;/td&gt;&lt;td&gt;&lt;p;&gt;
		inactive(1)Down
		l1Active(2)Layer 1 Up
		l2Active(3)Layer 2 Up
	&lt;/p&gt;&lt;/td;&gt;&lt;/tr&gt;&lt;/table&gt;
	</descr>
		<logmsg dest='logndisplay'>&lt;p&gt;
			Cisco Event: demandNbrLayer2Change trap received 
			ifIndex=%parm[#1]% 
			Line Status Down
		</logmsg>
 <severity>Critical</severity>
</event>
<!-- End of 5300dchan-PRI Down Trap -->

