      <parameter key="port" value="37" />
      <parameter key="allowed-skew" value="300" /><!-- Seconds, i.e. 5 minutes -->
      <parameter key="persist-skew" value="true" /><!-- If true, return a multi-valued PollStatus with skew included -->
    </service>
    <!--
