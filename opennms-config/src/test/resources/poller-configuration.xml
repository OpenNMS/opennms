<?xml version="1.0"?>
<poller-configuration threads="30"
                      serviceUnresponsiveEnabled="false"
                      nextOutageId="SELECT nextval('outageNxtId')">
  <node-outage status="on" pollAllIfNoCriticalServiceDefined="true">
    <critical-service name="ICMP" />
  </node-outage>
