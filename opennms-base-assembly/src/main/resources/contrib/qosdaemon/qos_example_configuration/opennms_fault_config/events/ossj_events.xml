<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
	<!-- OpenNMS OSS/J QoSD interface event. This is generated by vacuumd-configuration.xml -->
	<event>
		<uei>uei.opennms.org/vacuumd/alarmListChanged</uei>
		<event-label>
			OpenNMS-OSS/J inteface defined event: alarm List Changed
		</event-label>
		<descr>
			&lt;p&gt;This event is generated by
			vacuumd-configuration.xml when an alarm changes state
			&lt;/p&gt;
		</descr>
		<logmsg dest='donotpersist'>
			OpenNMS-OSS/J inteface defined event: alarm List Changed (
			could be 'logndisplay' or 'donotpersist')
		</logmsg>
		<severity>Indeterminate</severity>
	</event>

	<!-- *********************************** -->
	<!-- OpenNMS OSS/J interface test events -->
	<!-- *********************************** -->
		
	<event>
		<mask>
			<maskelement>
				<mename>id</mename>
				<mevalue>.1.3.6.1.4.1.5813.3</mevalue>
			</maskelement>
			<maskelement>
				<mename>generic</mename>
				<mevalue>6</mevalue>
			</maskelement>
			<maskelement>
				<mename>specific</mename>
				<mevalue>1</mevalue>
			</maskelement>
		</mask>
		<uei>uei.opennms.org/ossjTestEvent/newAlarm/1</uei>

		<event-label>OSSJ_TEST_ALARM_1_RAISE</event-label>

		<!-- discr maps to OSS/J AdditionalText -->
		<descr>OSSJ_TEST_ALARM_1</descr>

		<!-- logmsg maps to OSS/J SpecificProblem -->
		<logmsg dest='logndisplay'>OSSJ_TEST_ALARM_1</logmsg>

		<!-- severity maps to OSS/J PerceivedSeverity -->
		<severity>Critical</severity>

		<!-- operinstruct maps to OSS/J ProposedRepairActions -->		
		<operinstruct>Test1 ProposedRepairActions</operinstruct>
		
			<!-- (int) x733-probable-cause maps to OSS/J ProbableCause -->
			<!-- (string) x733-alarm-type maps to OSS/J AlarmType -->
			<!-- CommunicationsAlarm|ProcessingErrorAlarm|EnvironmentalAlarm|QualityOfServiceAlarm|EquipmentAlarm|IntegrityViolation|SecurityViolation|TimeDomainViolation|OperationalViolation|PhysicalViolation -->
		<alarm-data reduction-key="%uei%:%dpname%:%nodeid%"
			alarm-type="1" 
			x733-probable-cause="1"
			x733-alarm-type="CommunicationsAlarm" />
	</event>


	<event>
		<mask>
			<maskelement>
				<mename>id</mename>
				<mevalue>.1.3.6.1.4.1.5813.3</mevalue>
			</maskelement>
			<maskelement>
				<mename>generic</mename>
				<mevalue>6</mevalue>
			</maskelement>
			<maskelement>
				<mename>specific</mename>
				<mevalue>2</mevalue>
			</maskelement>
		</mask>
		<uei>uei.opennms.org/ossjTestEvent/clearAlarm/1</uei>
		<event-label>OSSJ_TEST_ALARM_1_CLEAR</event-label>

		<!-- discr maps to OSS/J AdditionalText -->
		<descr>OSSJ_TEST_ALARM_1</descr>

		<!-- logmsg maps to OSS/J SpecificProblem -->
		<logmsg dest='logndisplay'>OSSJ_TEST_ALARM_1</logmsg>

		<!-- severity maps to OSS/J PerceivedSeverity -->
		<severity>Cleared</severity>

		<!-- operinstruct maps to OSS/J ProposedRepairActions -->		
		<operinstruct>Test1 Cleared ProposedRepairActions</operinstruct>

		<alarm-data reduction-key="%uei%:%dpname%:%nodeid%"
			alarm-type="2"
			clear-uei="uei.opennms.org/ossjTestEvent/newAlarm/1" />
	</event>

</events>
