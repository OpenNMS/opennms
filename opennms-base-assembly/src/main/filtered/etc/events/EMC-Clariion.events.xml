      <descr>&lt;p>An EventMonitorTrap is generated in
response to a user-specified event.
Details can be found in Variables data.&lt;/p>&lt;table>
	&lt;tr>&lt;td>&lt;b>

	hostName&lt;/b>&lt;/td>&lt;td>
	%parm[#1]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	deviceID&lt;/b>&lt;/td>&lt;td>
	%parm[#2]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	eventID&lt;/b>&lt;/td>&lt;td>
	%parm[#3]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	eventText&lt;/b>&lt;/td>&lt;td>
	%parm[#4]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	storageSystem&lt;/b>&lt;/td>&lt;td>
	%parm[#5]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>&lt;/table></descr>
      <logmsg dest="logndisplay">&lt;p>
			EventMonitorTrap trap received 
			hostName=%parm[#1]% 
			deviceID=%parm[#2]% 
			eventID=%parm[#3]% 
			eventText=%parm[#4]% 
			storageSystem=%parm[#5]%&lt;/p>
		</logmsg>
      <severity>Indeterminate</severity>
   </event>
