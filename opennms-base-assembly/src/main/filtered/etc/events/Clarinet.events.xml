<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
   <event>
      <mask>
         <maskelement>
            <mename>id</mename>
            <mevalue>.1.3.6.1.4.1.2741.2</mevalue>
         </maskelement>
         <maskelement>
            <mename>generic</mename>
            <mevalue>6</mevalue>
         </maskelement>
         <maskelement>
            <mename>specific</mename>
            <mevalue>1</mevalue>
         </maskelement>
      </mask>
      <uei>uei.opennms.org/vendor/Clarinet/traps/heartBeat</uei>
      <event-label>CSI-P2-MIB defined trap event: heartBeat</event-label>
      <descr>&lt;p>This trap is the heart beat of the switch.&lt;/p>&lt;table>
	&lt;tr>&lt;td>&lt;b>

	swSerialNumber&lt;/b>&lt;/td>&lt;td>
	%parm[#1]%;&lt;/td>&lt;td>&lt;p;>&lt;/p>&lt;/td;>&lt;/tr>&lt;/table></descr>
      <logmsg dest="logndisplay">&lt;p>
			Clarinet Event: heartBeat trap received 
			swSerialNumber=%parm[#1]%&lt;/p>
		</logmsg>
      <severity>Indeterminate</severity>
   </event>
   <event>
      <mask>
         <maskelement>
            <mename>id</mename>
            <mevalue>.1.3.6.1.4.1.2741.2</mevalue>
         </maskelement>
         <maskelement>
            <mename>generic</mename>
            <mevalue>6</mevalue>
         </maskelement>
         <maskelement>
            <mename>specific</mename>
            <mevalue>2</mevalue>
         </maskelement>
      </mask>
      <uei>uei.opennms.org/vendor/Clarinet/traps/fatal</uei>
      <event-label>CSI-P2-MIB defined trap event: fatal</event-label>
      <descr>&lt;p>This trap indicating a fatal condition.  Device will reboot.&lt;/p>&lt;table>
	&lt;tr>&lt;td>&lt;b>

	swSerialNumber&lt;/b>&lt;/td>&lt;td>
	%parm[#1]%;&lt;/td>&lt;td>&lt;p;>&lt;/p>&lt;/td;>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	fatalMessage&lt;/b>&lt;/td>&lt;td>
	%parm[#2]%;&lt;/td>&lt;td>&lt;p;>&lt;/p>&lt;/td;>&lt;/tr>&lt;/table></descr>
      <logmsg dest="logndisplay">&lt;p>
			Clarinet Event: fatal trap received 
			swSerialNumber=%parm[#1]% 
			fatalMessage=%parm[#2]%&lt;/p>
		</logmsg>
      <severity>Indeterminate</severity>
   </event>
</events>