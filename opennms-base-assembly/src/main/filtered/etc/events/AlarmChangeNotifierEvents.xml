  <!-- generic test events to generate test alarms for AlarmChangeNotifier -->
  <!-- sudo ./send-event.pl  uei.opennms.org/plugin/AlarmChangeNotificationTest/TestAlarm/genericAlarmRaise localhost  -i 127.0.0.1 -p 'testParam 1' -->
  <!-- sudo ./send-event.pl  uei.opennms.org/plugin/AlarmChangeNotificationTest/TestAlarm/genericAlarmClear localhost  -i 127.0.0.1 -p 'testParam 1' -->
  <event>
    <uei>uei.opennms.org/plugin/AlarmChangeNotificationTest/TestAlarm/genericAlarmRaise</uei>
    <event-label>Generic AlarmChangeNotifier Alarm Raise %parm[testParam]%</event-label>
    <descr>Generic AlarmChangeNotifier Alarm Raise %parm[testParam]%</descr>
    <logmsg dest='logndisplay'>Generic AlarmChangeNotifier Alarm Raise %parm[testParam]%</logmsg>
    <severity>Minor</severity>
    <operinstruct></operinstruct>
    <alarm-data reduction-key="%uei%:%nodeid%:%interface%:%parm[testParam]%" alarm-type="1" auto-clean="false" />
  </event>

  <event>
    <uei>uei.opennms.org/plugin/AlarmChangeNotificationTest/TestAlarm/genericAlarmClear</uei>
    <event-label>Generic AlarmChangeNotifier Alarm Clear %parm[testParam]%</event-label>
    <descr>Generic AlarmChangeNotifier Alarm Clear %parm[testParam]%</descr>
    <logmsg dest='logndisplay'>Generic AlarmChangeNotifier Alarm Clear %parm[testParam]%</logmsg>
    <severity>Cleared</severity>
    <operinstruct></operinstruct>
    <alarm-data reduction-key="%uei%:%nodeid%:%interface%:%parm[testParam]%"
      clear-key="uei.opennms.org/plugin/AlarmChangeNotificationTest/TestAlarm/genericAlarmRaise:%parm[testParam]%"
      alarm-type="2"
      auto-clean="false" />
  </event>

</events>