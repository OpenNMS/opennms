<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
    <event>
        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.31023.1.1.1</mevalue>
            </maskelement>
            <maskelement>
                <mename>generic</mename>
                <mevalue>6</mevalue>
            </maskelement>
            <maskelement>
                <mename>specific</mename>
                <mevalue>1</mevalue>
            </maskelement>
        </mask>
        <uei>uei.opennms.org/mib2events/onBackupJobCompleted</uei>
        <event-label>VEEAM-MIB defined trap event: onBackupJobCompleted</event-label>
        <descr>&lt;p&gt;This trap is sent on backup/replica job completed.&lt;/p&gt;&lt;table&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	backupJobId&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#1]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	backupJobName&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#2]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	backupJobResult&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#3]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	backupJobComment&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#4]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;</descr>
        <logmsg dest="logndisplay">&lt;p&gt;
	onBackupJobCompleted trap received
	backupJobId=%parm[#1]%
	backupJobName=%parm[#2]%
	backupJobResult=%parm[#3]%
	backupJobComment=%parm[#4]%&lt;/p&gt;</logmsg>
        <severity>Indeterminate</severity>
    </event>
    <event>
        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.31023.1.1.1</mevalue>
            </maskelement>
            <maskelement>
                <mename>generic</mename>
                <mevalue>6</mevalue>
            </maskelement>
            <maskelement>
                <mename>specific</mename>
                <mevalue>2</mevalue>
            </maskelement>
        </mask>
        <uei>uei.opennms.org/mib2events/onVmBackupCompleted</uei>
        <event-label>VEEAM-MIB defined trap event: onVmBackupCompleted</event-label>
        <descr>&lt;p&gt;This trap is sent on vm backup/replica completed.&lt;/p&gt;&lt;table&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	backupJobName&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#1]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	vmName&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#2]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	sourceHostName&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#3]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	vmBackupResult&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#4]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	vmBackupComment&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#5]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;</descr>
        <logmsg dest="logndisplay">&lt;p&gt;
	onVmBackupCompleted trap received
	backupJobName=%parm[#1]%
	vmName=%parm[#2]%
	sourceHostName=%parm[#3]%
	vmBackupResult=%parm[#4]%
	vmBackupComment=%parm[#5]%&lt;/p&gt;</logmsg>
        <severity>Indeterminate</severity>
    </event>
</events>
