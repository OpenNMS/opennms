      <uei>uei.opennms.org/mib2events/onBackupJobCompleted</uei>
      <event-label>VEEAM-MIB defined trap event: onBackupJobCompleted</event-label>
      <descr>&lt;p>This trap is sent on backup/replica job completed.&lt;/p>&lt;table>
	&lt;tr>&lt;td>&lt;b>

	backupJobId&lt;/b>&lt;/td>&lt;td>
	%parm[#1]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	backupJobName&lt;/b>&lt;/td>&lt;td>
	%parm[#2]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	backupJobResult&lt;/b>&lt;/td>&lt;td>
	%parm[#3]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	backupJobComment&lt;/b>&lt;/td>&lt;td>
	%parm[#4]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>&lt;/table></descr>
      <logmsg dest="logndisplay">&lt;p>
	onBackupJobCompleted trap received
	backupJobId=%parm[#1]%
	backupJobName=%parm[#2]%
	backupJobResult=%parm[#3]%
	backupJobComment=%parm[#4]%&lt;/p></logmsg>
      <severity>Indeterminate</severity>
   </event>
