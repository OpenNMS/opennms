	&lt;tr>&lt;td>&lt;b>

	snaNodeOperName&lt;/b>&lt;/td>&lt;td>
	%parm[#1]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	snaNodeOperState&lt;/b>&lt;/td>&lt;td>
