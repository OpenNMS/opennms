<?xml version="1.0" encoding="UTF-8"?>
<database-schema xmlns="http://xmlns.opennms.org/xsd/config/filter">
    <table visible="true" name="node" key="secondary">
        <join type="inner" column="nodeID" table="ipInterface" table-column="nodeID"/>
        <column name="nodeID" visible="true"/>
        <column name="location" visible="true"/>
        <column name="nodeCreateTime" visible="true"/>
        <column name="nodeParentID" visible="true"/>
        <column name="nodeType" visible="true"/>
        <column name="nodeSysOID" visible="true"/>
        <column name="nodeSysName" visible="true"/>
        <column name="nodeSysDescription" visible="true"/>
        <column name="nodeSysLocation" visible="true"/>
        <column name="nodeSysContact" visible="true"/>
        <column name="nodeLabel" visible="true"/>
        <column name="nodeLabelSource" visible="true"/>
        <column name="nodeNetbiosName" visible="true"/>
        <column name="nodeDomainName" visible="true"/>
        <column name="operatingSystem" visible="true"/>
        <column name="foreignSource" visible="true"/>
        <column name="foreignID" visible="true"/>
    </table>
    <table visible="true" name="categories" key="secondary">
        <join type="left" column="categoryID" table="category_node" table-column="categoryID"/>
        <column name="categoryID" visible="true"/>
        <column name="categoryName" visible="true"/>
        <column name="categoryDescription" visible="true"/>
    </table>
    <table visible="true" name="category_node" key="secondary">
        <join type="left" column="nodeID" table="ipInterface" table-column="nodeID"/>
        <column name="categoryID" visible="false"/>
        <column name="nodeID" visible="false"/>
    </table>
    <table visible="true" name="ipInterface" key="primary">
        <join type="inner" column="snmpInterfaceId"
            table="snmpInterface" table-column="id"/>
        <column name="id" visible="false"/>
        <column name="nodeID" visible="false"/>
        <column name="ipAddr" visible="true"/>
        <column name="ipHostname" visible="true"/>
        <column name="IsManaged" visible="true"/>
        <column name="IsSnmpPrimary" visible="true"/>
        <column name="ipStatus" visible="true"/>
        <column name="ipLastCapsdPoll" visible="true"/>
    </table>
    <table visible="true" name="snmpInterface" key="secondary">
        <join type="inner" column="id" table="ipInterface" table-column="snmpinterfaceid"/>
        <column name="id" visible="false"/>
        <column name="nodeID" visible="false"/>
        <column name="snmpIpAdEntNetMask" visible="true"/>
        <column name="snmpPhysAddr" visible="true"/>
        <column name="snmpIfIndex" visible="true"/>
        <column name="snmpIfDescr" visible="true"/>
        <column name="snmpIfType" visible="true"/>
        <column name="snmpIfSpeed" visible="true"/>
        <column name="snmpIfAlias" visible="true"/>
        <column name="snmpIfAdminStatus" visible="true"/>
        <column name="snmpIfOperStatus" visible="true"/>
        <column name="snmpCollect" visible="true"/>
        <column name="snmpPoll" visible="true"/>
    </table>
    <table visible="true" name="service" key="secondary">
        <join type="inner" column="serviceID" table="ifServices" table-column="serviceID"/>
        <column name="serviceID" visible="false"/>
        <column name="serviceName" visible="true"/>
    </table>
    <table visible="true" name="ifServices" key="secondary">
        <join type="inner" column="ipInterfaceId" table="ipInterface" table-column="id"/>
        <join type="inner" column="id" table="application_service_map" table-column="ifServiceId"/>
        <column name="id" visible="false"/>
        <column name="ipAddr" visible="false"/>
        <column name="nodeID" visible="false"/>
        <column name="serviceID" visible="true"/>
        <column name="lastGood" visible="true"/>
        <column name="lastFail" visible="true"/>
    </table>
    <table visible="true" name="assets" key="secondary">
        <join type="inner" column="nodeID" table="ipInterface" table-column="nodeID"/>
        <column name="nodeID" visible="false"/>
        <column name="displayCategory" visible="true"/>
        <column name="notifyCategory" visible="true"/>
        <column name="pollerCategory" visible="true"/>
        <column name="thresholdCategory" visible="true"/>
        <column name="category" visible="true"/>
        <column name="manufacturer" visible="true"/>
        <column name="vendor" visible="true"/>
        <column name="modelnumber" visible="true"/>
        <column name="serialnumber" visible="true"/>
        <column name="description" visible="true"/>
        <column name="circuitid" visible="true"/>
        <column name="assetnumber" visible="true"/>
        <column name="operatingsystem" visible="true"/>
        <column name="rack" visible="true"/>
        <column name="slot" visible="true"/>
        <column name="port" visible="true"/>
        <column name="region" visible="true"/>
        <column name="division" visible="true"/>
        <column name="department" visible="true"/>
        <column name="address1" visible="true"/>
        <column name="address2" visible="true"/>
        <column name="city" visible="true"/>
        <column name="state" visible="true"/>
        <column name="zip" visible="true"/>
        <column name="country" visible="true"/>
        <column name="longitude" visible="true"/>
        <column name="latitude" visible="true"/>
        <column name="building" visible="true"/>
        <column name="floor" visible="true"/>
        <column name="room" visible="true"/>
        <column name="vendorphone" visible="true"/>
        <column name="vendorfax" visible="true"/>
        <column name="vendorassetnumber" visible="true"/>
        <column name="lease" visible="true"/>
        <column name="leaseexpires" visible="true"/>
        <column name="supportphone" visible="true"/>
        <column name="maintcontract" visible="true"/>
        <column name="maintcontractexpires" visible="true"/>
        <column name="comment" visible="true"/>
        <column name="managedobjectinstance" visible="true"/>
        <column name="managedobjecttype" visible="true"/>
    </table>
    <table visible="true" name="applications" key="secondary">
        <join type="inner" column="id" table="application_service_map" table-column="appId"/>
        <column name="id" visible="false"/>
        <column name="name" visible="true"/>
    </table>
    <table visible="true" name="application_service_map" key="secondary">
        <join type="inner" column="ifServiceId" table="ifServices" table-column="id"/>
        <join type="inner" column="appId" table="applications" table-column="id"/>
        <column name="appId" visible="false"/>
        <column name="ifServiceId" visible="false"/>
    </table>
</database-schema>
