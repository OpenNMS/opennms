      <mibObj oid=".1.3.6.1.4.1.14988.1.1.1.2.1.5" instance="mtxrWlRtabAddr" alias="mtxrWlRtabRxBytes" type="Counter32"/>
      <mibObj oid=".1.3.6.1.4.1.14988.1.1.1.2.1.6" instance="mtxrWlRtabAddr" alias="mtxrWlRtabTxPackets" type="Counter32"/>
      <mibObj oid=".1.3.6.1.4.1.14988.1.1.1.2.1.7" instance="mtxrWlRtabAddr" alias="mtxrWlRtabRxPackets" type="Counter32"/>
