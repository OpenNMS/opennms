      <mibObj oid=".1.3.6.1.4.1.14988.1.1.1.1.1.5" instance="mtxrWlStatIndex" alias="mtxrWlStatSsid" type="String"/>
      <mibObj oid=".1.3.6.1.4.1.14988.1.1.1.1.1.6" instance="mtxrWlStatIndex" alias="mtxrWlStatBssid" type="OctetString"/>
      <mibObj oid=".1.3.6.1.4.1.14988.1.1.1.1.1.7" instance="mtxrWlStatIndex" alias="mtxrWlStatFreq" type="Integer32"/>
   </group>
   <group name="mtxrWlRtabAddr" ifType="all">
      <mibObj oid=".1.3.6.1.4.1.14988.1.1.1.2.1.1" instance="mtxrWlRtabAddr" alias="mtxrWlRtabAddr" type="OctetString"/>
