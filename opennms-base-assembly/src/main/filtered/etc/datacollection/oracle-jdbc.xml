<datacollection-group name="Oracle-JDBC">

    <resourceType name="oraFailedLogins" label="Oracle Failed Logins" resourceLabel="${username}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collection.support.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.collection.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType name="oraAccountExp" label="Oracle Account Expiry" resourceLabel="${username}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collection.support.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.collection.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType name="oraTableSpace" label="Oracle Tablespaces" resourceLabel="${tablespace_name}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collection.support.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.collection.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType name="oraDBStats" label="Oracle Database Stats" resourceLabel="${database_name}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collection.support.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.collection.support.IndexStorageStrategy"/>
    </resourceType>

</datacollection-group>
