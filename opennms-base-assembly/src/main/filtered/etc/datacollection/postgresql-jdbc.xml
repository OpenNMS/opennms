<?xml version="1.0"?>
<datacollection-group name="PostgreSQL-JDBC">

    <resourceType name="pgTableSpace" label="PostgreSQL Tablespace" resourceLabel="${spcname}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collection.support.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.collection.support.IndexStorageStrategy"/>
    </resourceType>
    <resourceType name="pgDatabase" label="PostgreSQL Database" resourceLabel="${datname}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collection.support.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.collection.support.IndexStorageStrategy"/>
    </resourceType>

</datacollection-group>
