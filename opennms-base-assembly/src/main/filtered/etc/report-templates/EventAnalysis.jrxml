	WHERE
		events.eventuei = 'uei.opennms.org/generic/traps/EnterpriseDefault'
	GROUP BY
		nodeid,
		ipaddr,
		eventuei
	ORDER BY
		tally DESC
	LIMIT
		25)
	AS
		unknown_traps
LEFT JOIN
	node
ON
	(node.nodeid = unknown_traps.nodeid)]]>
