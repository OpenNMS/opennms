		<queryString>
			<![CDATA[SELECT
	tally,
	COALESCE(nodelabel,nodelabel,'<unknown>') AS node,
	COALESCE(unknown_traps.nodeid,unknown_traps.nodeid,null) AS nodeid,
	unknown_traps.eventuei,
	ipaddr
FROM
