	<queryString>
		<![CDATA[SELECT
	categories.categoryname,
	node.nodelabel,
	node.nodeid
FROM
	node
JOIN
	category_node
ON
	(category_node.nodeid = node.nodeid)
JOIN
	categories
ON
	(categories.categoryid = category_node.categoryid)
WHERE
	node.nodetype = 'A' AND
	categories.categoryname like $P{SURVEILLANCE_CATEGORY}
    AND node.nodelabel like $P{nameFilter}
ORDER BY
	categories.categoryname,
	node.nodelabel]]>
	</queryString>
