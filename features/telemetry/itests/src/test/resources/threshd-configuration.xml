<?xml version="1.0"?>
<threshd-configuration
   threads="5">

   <package name="thresholding-visitor-test">
       <filter>IPADDR != '0.0.0.0'</filter>
       <include-range begin="1.1.1.1" end="254.254.254.254"/>
       <service name="JTI-GPB" interval="300000" user-defined="false" status="on">
           <parameter key="thresholding-group" value="telemetry"/>
       </service>
   </package>

</threshd-configuration>

