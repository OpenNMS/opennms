<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:HTMLPanel >
        <g:HorizontalPanel styleName="search-token-field" ui:field="m_tokenContainer">
            <g:cell>
                <g:FlowPanel ui:field="m_namespace" styleName="search-token-namespace"></g:FlowPanel>
            </g:cell>
            <g:cell>
                <g:FlowPanel styleName="search-token-label" ui:field="m_label" />
            </g:cell>
            <g:cell>
                <g:HorizontalPanel styleName="search-token-ctrls" ui:field="m_iconPanel">
                    <g:Anchor ui:field="m_collapseBtn"></g:Anchor>
                    <g:Anchor ui:field="m_centerSuggestionBtn" styleName="icon-location-arrow"></g:Anchor>
                    <g:Anchor ui:field="m_closeBtn" styleName="icon-remove" ></g:Anchor>
                </g:HorizontalPanel>
            </g:cell>
        </g:HorizontalPanel>
    </g:HTMLPanel>


</ui:UiBinder>