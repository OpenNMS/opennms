<?xml version="1.0"?>
<syslogd-configuration>
	<configuration
		syslog-port="10514"
		new-suspect-on-message="false"
		parser="org.opennms.netmgt.syslogd.CustomSyslogParser"
		forwarding-regexp="^((.+?):? (.*))\n?$"
		matching-group-host="2"
		matching-group-message="3"
		discard-uei="DISCARD-MATCHING-MESSAGES" />
</syslogd-configuration>
