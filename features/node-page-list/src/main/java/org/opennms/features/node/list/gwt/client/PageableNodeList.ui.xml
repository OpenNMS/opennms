<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:opennms="urn:import:org.opennms.features.node.list.gwt.client">
	<ui:style >
		.tabLayoutStyle{
			
		}
	</ui:style>
	<g:TabLayoutPanel barUnit='EM' barHeight='3' ui:field="m_tabLayoutPanel" >
        <g:tab>
            <g:header size='7'>IP Interfaces</g:header>
            <g:LayoutPanel>
                <g:layer>
                    <opennms:IpInterfaceTable ui:field="m_ipInterfaceTable"/>
                </g:layer>
                <g:layer height="21px" bottom="2em" left="25%" right="25%">
                    <g:FlowPanel ui:field="m_ipTableDiv"/>
                </g:layer>
                <g:layer left="0%" right="70%" height="1.75em" bottom="0px">
                    <g:ListBox ui:field="m_ipSearchList"/>
                </g:layer>
                <g:layer left="30%" right="30%" height="1.75em" bottom="0px">
                    <g:TextBox ui:field="m_ipTextBox"/>
                </g:layer>
                <g:layer left="70%" right="0%" bottom="0px" height="1.75em">
                    <g:Button ui:field="m_ipSearchBtn">Search</g:Button>
                </g:layer>
            </g:LayoutPanel>
        </g:tab>
        <g:tab>
            <g:header size='7' style="font-family: Lucida Grande, Verdana, sans-serif;">Physical Interfaces</g:header>
            <g:LayoutPanel>
                <g:layer>
                    <opennms:PhysicalInterfaceTable ui:field="m_physicalInterfaceTable"/>
                </g:layer>
                <g:layer height="21px" bottom="1.75em" left="25%" right="25%">
                    <g:FlowPanel ui:field="m_physTableDiv"/>
                </g:layer>
                <g:layer left="0%" right="70%" height="1.75em" bottom="0px">
                    <g:ListBox ui:field="m_physSearchList"/>
                </g:layer>
                <g:layer left="30%" right="30%" height="1.75em" bottom="0px">
                    <g:TextBox ui:field="m_physTextBox"/>
                </g:layer>
                <g:layer left="70%" right="0%" bottom="0px" height="1.75em">
                    <g:Button ui:field="m_physSearchBtn">Search</g:Button>
                </g:layer>
            </g:LayoutPanel>
        </g:tab>
	</g:TabLayoutPanel>
</ui:UiBinder> 