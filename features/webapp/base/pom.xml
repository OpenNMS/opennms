<?xml version="1.0"?><project>
  <parent>
    <artifactId>org.opennms.feature.webapp</artifactId>
    <groupId>org.opennms.features</groupId>
    <version>1.7.5-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.opennms.features.webapp</groupId>
  <artifactId>org.opennms.features.webapp.base</artifactId>
  <packaging>war</packaging>
  <name>Opennms Base Webapp Container Module</name>
  <url>http://opennms.org</url>
  <build>
		<plugins>
		  <plugin>
	      	<groupId>org.codehaus.cargo</groupId>
	      	<artifactId>cargo-maven2-plugin</artifactId>
	      	<extensions>true</extensions>
	      	<executions>
	      		<execution>
	      			<goals>
	      				<goal>uberwar</goal>
	      			</goals>
	      		</execution>
	      	</executions>
	      	<configuration>
	      		<descriptor>src/assemble/merge.xml</descriptor>
	      		<container>
	      			<containerId>jetty6x</containerId>
	      			<type>embedded</type>
	      		</container>
	      	</configuration>
	      </plugin>
	      
<!--	      <plugin>-->
<!--        	<groupId>org.mortbay.jetty</groupId>-->
<!--        	<artifactId>maven-jetty-plugin</artifactId>-->
<!--        	<configuration>-->
<!--          		<webApp>${basedir}/target/org.opennms.features.webapp.base-${project.version}.war</webApp>-->
<!--       		</configuration>-->
<!--      	  </plugin>-->
	      
      </plugins>
      
  </build>
  
  <dependencies>
  	
  	<dependency>
  		<groupId>org.opennms</groupId>
  		<artifactId>opennms-webapp</artifactId>
  		<type>war</type>
  		<version>${project.version}</version>
  	</dependency>
  
  	<dependency>
  		<groupId>org.opennms.features.webapp</groupId>
  		<artifactId>org.opennms.features.webapp.home</artifactId>
  		<type>war</type>
  		<version>${project.version}</version>
  	</dependency>
  	
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
</project>