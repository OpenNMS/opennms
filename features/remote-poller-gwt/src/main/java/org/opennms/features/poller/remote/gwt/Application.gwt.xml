<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module>

  <!-- Various core dependencies -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.uibinder.UiBinder" />
  <inherits name="de.novanic.eventservice.GWTEventService"/>
  <inherits name="com.google.gwt.http.HTTP"/>
  <inherits name="com.google.gwt.json.JSON" />

  <!--  inherit css based theme -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>

  <!-- Loading Maps -->  
  <inherits name='com.google.gwt.ajaxloader.AjaxLoader' />
  <inherits name='com.google.gwt.maps.GoogleMaps' />
  <inherits name='com.google.gwt.maps.utility.GoogleMapsUtility' />
  <inherits name='com.googlecode.gwtmapquest.GWTMapquest' />
<!-- <inherits name='org.gwtopenmaps.openlayers.OpenLayers' /> -->

  <!-- Specify the application specific style sheet. -->
  <stylesheet src='Application.css' />

  <!-- Specify the app entry point class. -->
  <entry-point class='org.opennms.features.poller.remote.gwt.client.Application'/>

</module>
