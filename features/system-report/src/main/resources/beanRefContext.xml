<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">
        
  <bean id="systemReportContext" class="org.springframework.context.support.ClassPathXmlApplicationContext" lazy-init="true">
     <constructor-arg>
       <list>
         <value>classpath:/META-INF/opennms/applicationContext-systemReport.xml</value>
       </list>
     </constructor-arg>
     <constructor-arg ref="soaContext" />
     <constructor-arg ref="daoContext" />
   </bean>
   
</beans>
