    <!-- this context contains only the ConfigurationmanagerService, can be used when applicationContext-mockDao is not needed -->
    <context:annotation-config />
    <bean id="configurationManagerService" class="org.opennms.netmgt.dao.mock.ConfigurationManagerServiceMock" />

</beans>
