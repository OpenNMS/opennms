{
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"comma-dangle": [1, "never" ],
		"eqeqeq": 1,
		"quotes": [1, "single"],
		"no-cond-assign": 2,
		"no-console": 0,
		"no-constant-condition": 1,
		"no-control-regex": 2,
		"no-debugger": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 1,
		"no-empty": 1,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": 2,
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 1,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-sparse-arrays": 2,
		"no-unexpected-multiline": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-jsdoc": 1,
		"valid-typeof": 2
	}
}
