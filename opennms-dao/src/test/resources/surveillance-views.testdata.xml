<?xml version="1.0" encoding="UTF-8"?>
<surveillance-view-configuration 
  xmlns:this="http://www.opennms.org/xsd/config/surveillance-views" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  default-view="default">
  <views>
    <view name="default" >
      <rows>
        <row-def label="High Priority" >
          <category name="high"/>
        </row-def>
        <row-def label="Mid Priority" >
          <category name="mid" />
        </row-def>
        <row-def label="Low Priority" >
          <category name="low" />
        </row-def>
      </rows>
      <columns>
        <column-def label="Routers" >
          <category name="Routers" />
        </column-def>
        <column-def label="Swtiches" >
          <category name="Switches" />
        </column-def>
        <column-def label="Servers" >
          <category name="Servers" />
        </column-def>
      </columns>
    </view>
  </views>
</surveillance-view-configuration>
