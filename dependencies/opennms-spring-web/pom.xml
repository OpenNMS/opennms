<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <groupId>org.opennms</groupId>
    <artifactId>dependencies</artifactId>
    <version>1.7.5-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>opennms-spring-web</artifactId>
  <packaging>jar</packaging>
  <name>OpenNMS Spring Web Jars</name>
  <description>
    This module is used to provide a single artifact that the opennms project
    can depend on to use the spring library.
  </description>
  <!-- springVersion defined in the dependencies pom since its shared in multiple modules -->
  <dependencies>

    <!-- Spring -->

    <dependency>
      <groupId>org.springframework</groupId>
      <artifactId>spring-web</artifactId>
      <version>${springVersion}</version>
      <exclusions>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-beans</artifactId></exclusion>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-context</artifactId></exclusion>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-context-support</artifactId></exclusion>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-core</artifactId></exclusion>
      </exclusions>
    </dependency>

    <dependency>
      <groupId>org.springframework</groupId>
      <artifactId>spring-webmvc</artifactId>
      <version>${springVersion}</version>
      <exclusions>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-beans</artifactId></exclusion>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-context</artifactId></exclusion>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-context-support</artifactId></exclusion>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-core</artifactId></exclusion>
      </exclusions>
    </dependency>

    <dependency>
      <groupId>org.springframework</groupId>
      <artifactId>spring-webmvc-struts</artifactId>
      <version>${springVersion}</version>
      <exclusions>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-beans</artifactId></exclusion>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-context</artifactId></exclusion>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-context-support</artifactId></exclusion>
        <exclusion><groupId>org.springframework</groupId><artifactId>spring-core</artifactId></exclusion>
      </exclusions>
    </dependency>

    <!-- Spring Dependencies -->

    <dependency>
      <groupId>org.opennms</groupId>
      <artifactId>opennms-spring</artifactId>
    </dependency>

  </dependencies>
</project>
