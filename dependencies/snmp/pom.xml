<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <groupId>org.opennms</groupId>
    <artifactId>dependencies</artifactId>
    <version>1.9.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.opennms.dependencies</groupId>
  <artifactId>snmp-dependencies</artifactId>
  <packaging>jar</packaging>
  <name>OpenNMS SNMP Dependencies</name>
  <description>
    This module is used to provide a single artifact that the opennms project
    can depend on to use SNMP
  </description>
  <properties>
    <opennmsJoesnmpVersion>2.0.1</opennmsJoesnmpVersion>
    <opennmsSnmpApiVersion>2.0.1</opennmsSnmpApiVersion>
  </properties>


  <dependencies>
    <dependency>
      <groupId>org.opennms.lib</groupId>
      <artifactId>org.opennms.lib.joesnmp</artifactId>
      <version>${opennmsJoesnmpVersion}</version>
    </dependency>
    <dependency>
      <groupId>org.opennms.lib.snmp</groupId>
      <artifactId>org.opennms.lib.snmp.api</artifactId>
      <version>${opennmsSnmpApiVersion}</version>
    </dependency>
    <dependency>
      <groupId>org.opennms.lib.snmp</groupId>
      <artifactId>org.opennms.lib.snmp.joesnmp</artifactId>
      <version>${opennmsSnmpApiVersion}</version>
    </dependency>
    <dependency>
      <groupId>org.opennms.lib.snmp</groupId>
      <artifactId>org.opennms.lib.snmp.snmp4j</artifactId>
      <version>${opennmsSnmpApiVersion}</version>
    </dependency>
    <dependency>
      <groupId>org.snmp4j</groupId>
      <artifactId>snmp4j</artifactId>
      <version>1.11.1</version>
    </dependency>
  </dependencies>
  <repositories>
    <repository>
      <snapshots><enabled>false</enabled></snapshots>
      <releases><enabled>true</enabled></releases>
      <id>opennms-repo</id>
      <name>OpenNMS Repository</name>
      <url>http://repo.opennms.org/maven2</url>
    </repository>
    <repository>
      <snapshots><enabled>false</enabled></snapshots>
      <releases><enabled>true</enabled></releases>
      <id>snmp4j-repo</id>
      <name>SNMP4J Maven Repository</name>
      <url>https://server.oosnmp.net/dist/release</url>
    </repository>
  </repositories>
</project>
